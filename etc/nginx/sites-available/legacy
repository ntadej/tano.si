server {
	listen 80;
	listen [::]:80;
	listen 443 ssl http2;
	listen [::]:443 ssl http2;
	server_name discuss.tano.si;

	include /etc/nginx/snippets/letsencrypt.conf;

	return 301 https://tano.si/discussions/;

	access_log /var/log/nginx/legacy-discuss.access.log;
	error_log /var/log/nginx/legacy-discuss.error.log;
}
